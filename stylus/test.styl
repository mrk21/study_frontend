@import nib

mixin()
  background-color #fff

mixin_with_block()
  border 1px solid #000
  {block}

module-name_sub-module-name
  a = 200%
  b = { a: 1px }
  
  .&_class1 p.&
    font-size a
  
  #&_id1
    font-size b['a']

p
  mixin()
  +mixin_with_block()
    color #fff
  font-size 200%
  background linear-gradient(top, white, black)
  box-shadow 1px 2px 5px #eee
